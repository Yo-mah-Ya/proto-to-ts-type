{
  "name": "proto-to-ts-type",
  "version": "1.0.0",
  "description": "proto-to-ts-type",
  "author": "Yoshinari-Yamanaka",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -b && ./post_build.sh",
    "test": "jest --coverage --passWithNoTests --maxWorkers=7",
    "proto": "yarn proto:descriptor && yarn proto:plugin",
    "proto:descriptor": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. src/protobuf/descriptor.proto",
    "proto:plugin": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. src/protobuf/plugin.proto"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.7",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "google-protobuf": "^3.21.2",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-proto": "^1.162.1",
    "typescript": "^5.2.2"
  }
}
